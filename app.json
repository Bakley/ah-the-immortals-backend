{
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "DATABASE_HOST": {
      "required": true
    },
    "DATABASE_NAME": {
      "required": true
    },
    "DATABASE_PASSWORD": {
      "required": true
    },
    "DATABASE_USER": {
      "required": true
    },
    "DISABLE_COLLECTSTATIC": {
      "required": true
    },
    "DJANGO_ALLOWED_HOSTS": {
      "required": true
    },
    "DJANGO_SECRET_KEY": {
      "required": true
    },
    "DOMAIN": {
      "required": true
    },
    "JWT_SECRET_KEY": {
      "required": true
    },
    "SWAGGER_URL": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "ah-the-immortals-backend",
  "scripts": {
  },
  "stack": "heroku-18"
}
